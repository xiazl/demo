<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.fly.caipiao.analysis.mapper.ErrorRecordMapper">
   <resultMap id="BaseResultMap" type="com.fly.caipiao.analysis.entity.User" >
       <id column="id" property="id" />
       <result column="username" property="username"/>
       <result column="password" property="password"/>
       <result column="cellphone" property="cellphone"/>
       <result column="status_flag" property="statusFlag"/>
       <result column="create_time" property="createTime"/>
   </resultMap>

    <insert id="insert" parameterType="com.fly.caipiao.analysis.entity.ErrorRecord">
        insert into `error_record` (`name`,`time`,status,create_time)
        values (#{name},#{time},#{status},#{createTime})
    </insert>

</mapper>
